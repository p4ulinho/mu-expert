{
  "name": "multi-contas-expert-ps",
  "private": true,
  "version": "0.0.6",
  "description": "Gerencie milhares de contas de forma facilitada e eficiente",
  "author": "",
  "license": "Multi Expert PS",
  "type": "module",
  "main": "dist-electron/main.js",
  "scripts": {
    "dev": "vite",
    "dev:all": "npx -y concurrently -k \"npm run dev --prefix api\" \"npm run dev\"",
    "build": "tsc && vite build && electron-builder",
    "preview": "vite preview",
    "electron:dev": "vite",
    "electron:build": "vite build && electron-builder",
    "start": "electron . --disable-gpu-shader-disk-cache --disable-disk-cache"
  },
  "dependencies": {
    "axios": "^1.13.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.3",
    "electron-log": "^5.4.3",
    "electron-updater": "^6.6.2",
    "framer-motion": "^12.23.26",
    "lucide-react": "^0.562.0",
    "playwright": "^1.57.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-hot-toast": "^2.6.0",
    "react-router-dom": "^6.30.2",
    "tailwind-merge": "^3.4.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@tailwindcss/vite": "^4.1.18",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "autoprefixer": "^10.4.23",
    "dotenv-cli": "^11.0.0",
    "electron": "^39.2.7",
    "electron-builder": "^26.0.12",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.18",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4",
    "vite-plugin-electron": "^0.29.0",
    "vite-plugin-electron-renderer": "^0.14.6"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/p4ulinho/mu-expert.git"
  },
  "build": {
    "extends": null,
    "appId": "com.multicontas.expertps",
    "productName": "Multi Contas Expert PS",
    "directories": {
      "output": "release_new/${version}",
      "app": "."
    },
    "publish": [
      {
        "provider": "github",
        "owner": "p4ulinho",
        "repo": "mu-expert"
      }
    ],
    "files": [
      "dist",
      "dist-electron"
    ],
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ]
    },
    "mac": {
      "target": [
        "dmg",
        "zip"
      ],
      "category": "public.app-category.utilities"
    }
  }
}